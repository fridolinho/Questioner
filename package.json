  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "coveralls": "^3.0.2",
    "express": "^4.16.4",
    "joi": "^14.3.1",
    "nodemon": "^1.18.9",
    "nyc": "^13.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
