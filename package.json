  "directories": {
    "test": "test"
  },
  "engines": {
    "node": "10.15.0"
  },
  "scripts": {
    "start": "nodemon --exec babel-node --presets env app",
    "test": "nyc --reporter=html --reporter=text mocha --timeout 10000 --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
